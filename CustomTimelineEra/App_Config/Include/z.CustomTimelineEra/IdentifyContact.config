<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <settings>
      <setting name="CustomTimelineEra.ContactEmail" value="bruce.wayne@wayneenterprises.com" />
    </settings>
    
    <pipelines>
      <createVisit>
        <processor type="CustomTimelineEra.Pipelines.CreateVisit.IdentifyContact, CustomTimelineEra"
                   patch:before="processor[1]" />
        <processor type="CustomTimelineEra.Pipelines.CreateVisit.UpdateContactInformation, CustomTimelineEra"
                   patch:after="processor[@type='Sitecore.Analytics.Pipelines.CreateVisits.InitializeProfile, Sitecore.Analytics']" />
      </createVisit>
    </pipelines>
  </sitecore>
</configuration>
